<div class="card">
    <div class="card-header">
        <h1>{{model.series_name}}</h1>
        <h3>{{model.post.title}}</h3>
    </div>
    <div class="card-body">
        {{{model.post.body}}}
    </div>
    <div class="card-footer">
        {{bootstrap-paginate links=model.links series_name=model.series_name post_title=model.post.title}}
    </div>
</div>
{{outlet}}