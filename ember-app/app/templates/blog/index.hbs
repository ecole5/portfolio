

  <div class="card mb-5 br-lg">

    <div class="card-body">
      <div class="row">
        {{#each model as |post |}}
          <div class="col-md">
            <div class="card mb-5">
              <div class="card-body">
                <h5 class="card-title">{{post.title}}</h5>
                  <h5 class="card-title">{{post.date}}</h5>
                  {{{post.content}}}
                {{#link-to "blog.post" post.title }}Read More{{/link-to}}
              </div>
              
            </div>
          </div>
        {{/each}}
      </div>
    </div>
  </div>

{{outlet}}